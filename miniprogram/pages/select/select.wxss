page{
    width: 100%;
    height: 100%;
}
.container{
    width: 100%;
    height: 100%;
}
scroll-view ::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
    display: none;
}
.nav {
    height: 80rpx;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    line-height: 80rpx;
    background: #f7f7f7;
    font-size: 16px;
    white-space: nowrap;
}
.navTwo {
    height: 80rpx;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    line-height: 80rpx;
    background: #f7f7f7;
    font-size: 12px;
    white-space: nowrap;
}
.content {
    margin-top: 2rpx;
}
.nav-item1 {
    width: 20%;
    display: inline-block;
    text-align: center;
}
.nav-item2 {
    width: 20%;
    height: 60rpx;
    line-height: 60rpx;
    display: inline-block;
    text-align: center;
}
.nav .active{
    color: #FE5D26;
}
.navTwo .active{
    color: #0FA2E8;
    border-bottom: 1rpx solid #0FA2E8;
    animation: moveBottom 2s linear;
}
.tab-box{
    height: 100%;
    margin-top: 10rpx;
    box-sizing: border-box;
}
.page-group-position {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
}
@keyframes moveBottom {
    0% {
        transform: translateX(5%);
    }
    50% {
        transform: translateX(10%);
    }
    75% {
        transform: translateX(-10%);
    }
    100% {
        transform: translateX(-5%);
    }
}